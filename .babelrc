{
  "presets": [
    [
      "@babel/env",
      {
        "shippedProposals": true,
        "useBuiltIns": "usage",
        "targets": {
          "node": "12.14"
        },
        "modules": "cjs",
        "corejs": {
          "version": 3
        }
      }
    ]
  ],

  // Optional Plugins -> Proposals. Property does have to be used. Plugins have to be installed via npm.
  "plugins": [
    ["@babel/plugin-proposal-pipeline-operator", { "proposal": "minimal" }]
  ]
}
